# Algalon Worker Configuration
# Copy this file to .env and modify as needed

# all-smi version to build and use
ALL_SMI_VERSION=v0.9.0

# Port for all-smi API endpoint
ALL_SMI_PORT=9090

# Examples for different configurations:
#
# For older version:
# ALL_SMI_VERSION=v0.8.0
#
# For custom port (remember to update host targets accordingly):
# ALL_SMI_PORT=8080
#
# For latest development version:
# ALL_SMI_VERSION=main